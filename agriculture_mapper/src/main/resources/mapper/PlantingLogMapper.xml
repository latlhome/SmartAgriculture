<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.agriculture.mapper.PlantingLogMapper">

    <select id="selectPlantingLogById" resultType="com.smart.agriculture.Do.PlantingLog">
        select *
        from planting_log
        where id = #{id} and
              is_del = 0
    </select>
</mapper>
